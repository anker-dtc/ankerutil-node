{
  "name": "ankerutil-node",
  "version": "1.0.0",
  "description": "Node.js 敏感数据加密工具库 - 支持 NestJS 7.x/9.x 和 Express.js 4.x",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest",
    "test:compatibility": "npx ts-node examples/multi-framework-compatibility.ts",
    "test:nodejs": "npx ts-node examples/nestjs-integration-example.ts",
    "clean": "rm -rf dist node_modules package-lock.json",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "keywords": [
    "encryption",
    "decryption",
    "sensitive-data",
    "nestjs",
    "express",
    "nodejs",
    "typescript",
    "aes",
    "sha256"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chai": "^4.3.10",
    "nacos": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">=7.0.0",
    "express": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "express": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ankerutil-node.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ankerutil-node/issues"
  },
  "homepage": "https://github.com/your-org/ankerutil-node#readme"
}
